{
    "version": 2,
    "git": {
        "deploymentEnabled": {
            "main": false
        }
    },
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}
